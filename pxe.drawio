        <mxCell id="IpQB33nou2Lp-JkqVrUY-9" value="[Unit]&lt;br&gt;Description=wol-reboot-triiger&lt;br&gt;After=network.target&lt;br&gt;Requires=network.target&lt;br&gt;&lt;br&gt;&lt;br&gt;[Service]&lt;br&gt;Type=simple&lt;br&gt;ExecStart=/bin/bash -c &#39;while true; do tcpdump -i any -n -c 1 &quot;udp port 9 and src host &lt;b&gt;указать ip отправителя пакета&lt;/b&gt;&quot; &amp;amp;&amp;amp; reboot; done&#39;&lt;br&gt;User=root&lt;br&gt;Restart=always&lt;br&gt;RestartSec=5&lt;br&gt;&lt;br&gt;[Install]&lt;br&gt;WantedBy=multi-user.target" style="text;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;spacingLeft=4;spacingRight=4;overflow=hidden;points=[[0,0.5],[1,0.5]];portConstraint=eastwest;rotatable=0;whiteSpace=wrap;html=1;" vertex="1" parent="IpQB33nou2Lp-JkqVrUY-8">
